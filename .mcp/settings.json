{
  "mcpServers": {
    "postgres": {
      "command": "uvx",
      "args": ["mcp-server-postgres"],
      "env": {
        "POSTGRES_CONNECTION_STRING": "postgresql://cs2_user:cs2_password@localhost:5432/cs2_analysis"
      }
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/home/runner/work/fps-genie/fps-genie"]
    },
    "brave-search": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-brave-search"],
      "env": {
        "BRAVE_API_KEY": "${BRAVE_API_KEY}"
      }
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      }
    },
    "rust-analyzer": {
      "command": "rust-analyzer",
      "args": ["--stdio"],
      "env": {
        "RUST_BACKTRACE": "1"
      }
    },
    "database-inspector": {
      "command": "node",
      "args": ["/home/runner/work/fps-genie/fps-genie/.mcp-servers/database-inspector.js"],
      "env": {
        "DATABASE_URL": "postgresql://cs2_user:cs2_password@localhost:5432/cs2_analysis",
        "TIMESCALE_URL": "postgresql://cs2_user:cs2_password@localhost:5432/cs2_analysis",
        "QDRANT_URL": "http://localhost:6333",
        "REDIS_URL": "redis://localhost:6379"
      }
    },
    "demo-analyzer": {
      "command": "node",
      "args": ["/home/runner/work/fps-genie/fps-genie/.mcp-servers/demo-analyzer.js"],
      "env": {
        "DEMO_DIR": "/home/runner/work/fps-genie/fps-genie/test_data",
        "WORKSPACE_ROOT": "/home/runner/work/fps-genie/fps-genie"
      }
    },
    "cargo-helper": {
      "command": "node",
      "args": ["/home/runner/work/fps-genie/fps-genie/.mcp-servers/cargo-helper.js"],
      "env": {
        "WORKSPACE_ROOT": "/home/runner/work/fps-genie/fps-genie"
      }
    }
  }
}